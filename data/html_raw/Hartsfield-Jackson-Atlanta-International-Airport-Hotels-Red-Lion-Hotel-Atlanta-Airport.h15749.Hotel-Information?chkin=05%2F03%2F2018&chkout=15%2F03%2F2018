<script type="text/javascript" defer>
require(['uitk', 'jquery', 'handlebars'], function(uitk, $, Handlebars) {
var isTaapUser = undefined,
storedViews = [],
taapInfo = {},
scriptLoaded = false;
var publishStoredEvents = function() {
uitk.publish('taap-ui-global-inject', { abacus: taapInfo.abacus, userInfo: taapInfo.userInfo });
storedViews.forEach(function(singleView) {
uitk.publish('taap-ui-roomrate-inject', { view: singleView, abacus: taapInfo.abacus, userInfo: taapInfo.userInfo });
});
storedViews = [];
};
var scriptPath = getParameterByName("taapSoak") === "yes"? 'https://s3.amazonaws.com/taap-ui-bundles-test/taapHISBundle.js' : 'https://d4rb83r2tty9a.cloudfront.net/taapHISBundle.js';
$.ajax({
url: '/taap-abacus',
type: 'GET',
data: {pageName: 'page.Hotels.Infosite.Information'},
dataType: 'json',
contentType: 'application/json; charset=UTF-8',
success: function(data) {
taapInfo = data;
if (!$.isEmptyObject(taapInfo)) {
isTaapUser = true;
$LAB.script(scriptPath).wait(function() {
scriptLoaded = true;
publishStoredEvents();
});
} else {
isTaapUser = false;
}
}
});
var offerRenderToken = uitk.subscribe('offerview:render', function (topic, view) {
if (view === undefined || view.model === undefined || view.model.attributes === undefined) return;
if (isTaapUser && scriptLoaded) {
uitk.publish('taap-ui-roomrate-inject', { view: view, abacus: taapInfo.abacus, userInfo: taapInfo.userInfo });
} else if (isTaapUser === false) {
uitk.unsubscribe('offerview:render', offerRenderToken);
} else {
storedViews.push(view);
}
});
});
function getParameterByName(name, url) {
if (!url) {
url = window.location.href;
}
name = name.replace(/[\[\]]/g, "\\$&");
var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
results = regex.exec(url);
if (!results) return null;
if (!results[2]) return '';
return decodeURIComponent(results[2].replace(/\+/g, " "));
}
