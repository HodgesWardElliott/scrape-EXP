<script type="text/javascript">
+function() {
"use strict";
setTimeout(actionEntry, 10000);
var thresholdTimeout,
clicksInThisClickGroup = 0,
clicksThreshold = 7,
maxDelayBetweenClicks = 500,
textareaId = "customer-debug-textarea",
debugInfoContainerId = "debug-info-container",
siteFooterWrapId = 'site-footer-wrap',
requireExists = (typeof window.require === 'function') ? true : false;
function actionEntry() {
document.body.onclick = bodyClick;
}
function bodyClick(e) {
var ignoredElements = ['A', 'BUTTON', 'INPUT', 'SELECT', 'TEXTAREA'];
if(e.target && ignoredElements.indexOf(e.target.nodeName) > -1) {
return;
}
if(e.defaultPrevented) {
return;
}
if(thresholdTimeout) {
clearTimeout(thresholdTimeout);
}
thresholdTimeout = setTimeout(clearClicks, maxDelayBetweenClicks);
clicksInThisClickGroup++;
if(clicksInThisClickGroup >= clicksThreshold) {
collectAndDisplayDebugInfo();
}
}
function collectAndDisplayDebugInfo() {
var debugInfo = {};
debugInfo.dateAndTime = new Date();
if(typeof debugInfo.dateAndTime.toLocaleDateString === 'function') {
debugInfo.localeDate = debugInfo.dateAndTime.toLocaleDateString();
}
if(typeof debugInfo.dateAndTime.toLocaleTimeString === 'function') {
debugInfo.localeTime = debugInfo.dateAndTime.toLocaleTimeString();
}
if(typeof debugInfo.dateAndTime.toGMTString === 'function') {
debugInfo.GmtDate = debugInfo.dateAndTime.toGMTString();
}
debugInfo.url = window.location && window.location.href ? window.location.href : undefined;
if(window.navigator) {
debugInfo.browserName = window.navigator.appName;
debugInfo.browserVersion = window.navigator.appVersion;
debugInfo.userAgent = window.navigator.userAgent;
debugInfo.cookieEnabled = window.navigator.cookieEnabled;
debugInfo.version = window.navigator.appVersion;
debugInfo.languages = window.navigator.languages;
}
var OSName = "Unknown OS";
if(window.navigator && window.navigator.appVersion) {
if(window.navigator.appVersion.indexOf("Win") != -1) { OSName = "Windows"; }
if(window.navigator.appVersion.indexOf("Mac") != -1) { OSName = "MacOS"; }
if(window.navigator.appVersion.indexOf("X11") != -1) { OSName = "UNIX"; }
if(window.navigator.appVersion.indexOf("Linux") != -1) { OSName = "Linux"; }
debugInfo.OSName = OSName;
}
if(window.dctk && window.dctk.logging) {
debugInfo.guid = window.dctk.logging.guid;
// override with more relevant information
debugInfo.browserName = window.dctk.logging.browser;
debugInfo.OSName = window.dctk.logging.OS;
}
if(location.href.match(/\/hotel-search/i)) {
HSRSpecificInfo(debugInfo);
}
if(location.href.match(/\.hotel-information/i)) {
HISSpecificInfo(debugInfo);
}
displayDebugInfo(debugInfo);
}
function HSRSpecificInfo(debugInfo) {
if(window.pageModel) {
debugInfo.allExperiments = window.pageModel.allExperiments;
if(window.pageModel.wizard) {
debugInfo.wizard = {
checkIn: window.pageModel.wizard.checkIn,
checkOut: window.pageModel.wizard.checkOut
};
}
debugInfo.isLoggedInUser =
window.pageModel.traveler ? window.pageModel.traveler.isLoggedIn : 'traveler object not found';
debugInfo.isRegisteredUser =
window.pageModel.traveler ? window.pageModel.traveler.isRegistered
: 'traveler object not found';
}
}
function HISSpecificInfo(debugInfo) {
if(requireExists) {
var urlModelFallBack = function() {
return {
getCheckIn: function() { return 'urlModel not found'; },
getCheckOut: function() { return 'urlModel not found'; }
}
},
experiments = require('experiments') || 'experiments not found',
UrlModel = require('urlModel') || urlModelFallBack,
urlModel = new UrlModel(),
infositeData = require('infositeData') || 'infositeData not found';
debugInfo.allExperiments = experiments;
debugInfo.wizard = {
checkIn: urlModel.getCheckIn(),
checkOut: urlModel.getCheckOut()
};
debugInfo.isLoggedInUser = infositeData.isAuthenticatedUser;
debugInfo.isRegisteredUser = infositeData.isRegisteredUser;
}
}
function displayDebugInfo(debugInfo) {
var debugContainer = document.getElementById(debugInfoContainerId) || document.createElement('details');
debugContainer.innerHTML = '';
var summary = document.createElement('summary');
summary.innerText = "Debug information";
debugContainer.appendChild(summary);
debugContainer.setAttribute('id', debugInfoContainerId);
debugContainer.setAttribute('style', 'margin-top: 1em; margin-bottom: 2em;');
var siteFooterWrap = document.getElementById(siteFooterWrapId);
if(siteFooterWrap) {
siteFooterWrap.appendChild(debugContainer);
} else {
document.body.appendChild(debugContainer);
}
var textarea = document.getElementById(textareaId);
var debugInfoText = "============ debug information begins ==============\n\n";
debugInfoText += JSON.stringify(debugInfo, null, 4) + "\n\n";
debugInfoText += "============ debug information ends ==============";
if(!textarea) {
textarea = document.createElement('textarea');
textarea.setAttribute('rows', '10');
textarea.setAttribute('id', textareaId);
textarea.setAttribute('style', 'width: 100%; color: #fff; background-color: #222;');
debugContainer.appendChild(textarea);
createButton({
caption: 'Copy info to clipboard',
class: 'btn-secondary btn-action',
container: debugContainer,
clickHandler: copyClicked
});
createButton({
caption: 'Select all',
class: 'btn-secondary btn-action',
container: debugContainer,
clickHandler: selectAllClicked
});
createButton({
caption: 'Close window',
class: 'btn-secondary btn-utility',
container: debugContainer,
clickHandler: removeDebugInfoClicked
});
}
textarea.value = debugInfoText;
if(textarea.getAttribute('data-alertshown') !== 'true') {
textarea.setAttribute('data-alertshown', 'true');
}
logMessage('customerDebugInfoShown');
}
function selectAllClicked() {
var textarea = document.getElementById(textareaId);
if(!textarea) { return; }
textarea.select();
if(typeof textarea.setSelectionRange === 'function') {
var length = 99999999999;
if(textarea.textLength && textarea.textLength > 0) {
textarea.setSelectionRange(0, textarea.textLength);
} else if(textarea.textLength === undefined) {
textarea.setSelectionRange(0, length);
}
}
}
function createButton(buttonInfo) {
var button = document.createElement('button');
button.setAttribute('style', 'margin-top: 1em;');
button.setAttribute('class', buttonInfo.class);
button.setAttribute('type', 'button');
var childSpan = document.createElement('span');
childSpan.setAttribute('class', 'btn-label');
childSpan.appendChild(document.createTextNode(buttonInfo.caption));
button.appendChild(childSpan);
button.onclick = buttonInfo.clickHandler;
buttonInfo.container.appendChild(button);
}
function clearClicks() {
clicksInThisClickGroup = 0;
}
function removeDebugInfoClicked() {
var debugInfoContainer = document.getElementById(debugInfoContainerId);
if(!debugInfoContainer) { return; }
var siteFooterWrap = document.getElementById(siteFooterWrapId);
if(siteFooterWrap) {
siteFooterWrap.removeChild(debugInfoContainer);
} else {
document.body.removeChild(debugInfoContainer);
}
}
function copyClicked(e) {
selectAllClicked();
try {
var successful = document.execCommand('copy', true);
if(successful) {
window.alert('Info copied to the clipboard');
} else {
throw new Error();
}
} catch(err) {
// unable to copy to clipboard
window.alert('Failed to copy debug info to the clipboard');
logMessage('debugInfoCopyToClipboardFailed');
}
}
function logMessage(message) {
if(window.require && typeof window.require === 'function') {
require(['logger'], function(logger) {
logger.logMessage(message);
});
}
}
}();
</script>
