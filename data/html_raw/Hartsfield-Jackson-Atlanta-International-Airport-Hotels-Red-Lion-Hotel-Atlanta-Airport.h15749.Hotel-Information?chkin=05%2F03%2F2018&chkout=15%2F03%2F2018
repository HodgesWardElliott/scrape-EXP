define('templates', [
'handlebars', 'experiments', 'infosite', 'reviews', 'handlebarsHelpers'
], function(Handlebars, experiments, infosite, reviews){
var templates = Handlebars.templates = Handlebars.templates || {};
templates['reviews-template'] = Handlebars.compile('<section id=\"reviews\" class=\"segmented-list cf\" > {{#each reviews}} <article class=\"segment no-target review cf translate-original\" data-review-id=\"{{reviewId}}\" data-content-locale=\"{{contentLocale}}\"><div itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\"><div class=\"summary\"><h3 class=\"visuallyhidden\" itemprop=\"name\"> {{#ifHasContent title}}{{{title}}}{{/ifHasContent}}<\/h3><span class=\"rating\"> {{locH \'ratingOutOf5\' bundle=\'templates\'}} <span class=\"flag-corner\"><\/span><\/span><blockquote class=\"recommendation {{#if recommended}}recommends{{/if}}\"> {{#if recommended}} <span class=\"icon icon-success\"><\/span> {{#ifHasCategoryLabel reviewerCategories}} {{locH \'reviewRecommendedForCategory\' bundle=\'templates\'}} {{else}} {{locH \'reviewRecommended\' bundle=\'templates\'}} {{/ifHasCategoryLabel}} {{/if}} <div class=\"user cf\"> {{#ifHasContent userNickname}} {{locH \'rsByUser\' bundle=\'templates\'}} {{else}} {{locH \'rsByAVerifiedTraveler\' bundle=\'templates\'}} {{/ifHasContent}} {{#ifHasContent userLocation}} {{locH \'fromLocation\' bundle=\'templates\'}} {{/ifHasContent}} <\/div><\/blockquote><\/div><div class=\"details\"><h3 aria-hidden=\"true\">{{#ifHasContent title}}{{{title}}}{{/ifHasContent}}<\/h3> {{#if ratingOnlyReview}} <span class=\"rating-score-only\"> {{locH \'ratingScoreOnly\' bundle=\'templates\'}} <\/span> {{/if}} <span class=\"date-posted\"> {{#if reviewSubmissionTime}} {{locH \'localizedDatePosted\' bundle=\'templates\'}} {{/if}} {{#unless isCurrentBrand}}{{locH \'onBrandType\' bundle=\'templates\'}}{{/unless}} <\/span> {{#ifHasContent positiveRemarks}} <div class=\"remark\" tabindex=\"0\"><strong>{{locH \'pros\' bundle=\'templates\'}}<\/strong><span class=\"translate-pros\"> {{{positiveRemarks}}}<\/span><\/div> {{/ifHasContent}} {{#ifHasContent negativeRemarks}} <div class=\"remark\" tabindex=\"0\"><strong>{{locH \'cons\' bundle=\'templates\'}}<\