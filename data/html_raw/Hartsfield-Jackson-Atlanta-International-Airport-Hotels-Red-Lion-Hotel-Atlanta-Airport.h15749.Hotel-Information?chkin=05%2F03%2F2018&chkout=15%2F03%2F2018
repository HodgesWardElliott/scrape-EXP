<script>
/*<script type="text/javascript">*/
define('performanceLogger', [], function() {
"use strict";
var performanceLogger = {
mark: function() {},
measure: function() {},
log: function() {},
restart: function() {},
publish: function() {}
};
if(window.performance && window.performance.timing && window.performance.now) {
window.hasPerf = false;
if(window.performance.clearMarks){
window.hasPerf = true;
}
performanceLogger = {
events: {},
mark: function(marker)
{
var now = window.performance.now();
this.setMark(marker, now);
if (window.hasPerf) {
window.performance.mark(marker);
}
if (this.isDebugMode() && window.console){
window.console.log("[PERF] " + marker +" at "+ now.toFixed(1)+"ms");
}
},
measure: function(marker)
{
var now = window.performance.now();
var event = this.events[marker];
if(!event) {
this.events[marker] = this.setMark(marker, now);
}
else if(!event.end) {
event.end = now;
event.duration = event.end- event.start;
}
if(window.hasPerf) {
var markers = window.performance.getEntriesByName(marker + "-start");
if (markers && markers.length > 0) {
window.performance.mark(marker + "-end");
window.performance.measure(marker, marker + "-start", marker + "-end");
if (this.isDebugMode() && window.console) {
this.log(marker);
}
}
else {
window.performance.mark(marker + "-start");
}
}
},
log: function(marker)
{
var delta;
if (window.hasPerf) {
var measure = window.performance.getEntriesByName(marker);
if(measure && measure[0]){
delta = measure[0].duration;
}
}
else{
if (this.events[marker] && this.events[marker].duration){
delta = this.events[marker].duration;
}
}
if (delta && window.console){
window.console.log('[PERF] ' + marker + " took " + delta.toFixed(1) + "ms");
}
},
setMark: function(id, time) {
this.events[id] = {
id: id,
start: time
};
return this.events[id];
},
isDebugMode: function(){
return window.location.search.indexOf('bundling-debug') > -1 ? true : false;
},
restart: function() {
this.events = {};
if(window.hasPerf){
window.performance.clearMarks();
}
},
publish: function() {
var that = this;
require('dctk', function(dctk) {
var event, params, key;
if(dctk.loggingAdapter) {
for(key in that.events) {
event = that.events[key];
if (event.duration) {
params = [];
params.push('clEventName01=' + event.id);
params.push('clEventTime01=' + event.duration);
dctk.loggingAdapter.logTrxEvent('navTiming', params);
}
}
}
});
}
};
}
return performanceLogger;
});</script>
